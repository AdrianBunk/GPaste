msgstr "corrige a extensão do gnome-shell quando o daemon é reiniciado"

#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:68
msgid "Fix keybindings for wayland"
msgstr "Corrige associações de teclas para o wayland"

#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:75
msgid "First release for GNOME 41. Changes:"
msgstr "Primeiro lançamento para o GNOME 41. Alterações:"

#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:77
msgid "port to gnome-shell 41"
msgstr "port para gnome-shell 41"

#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:94
msgid "fix a crash in memory usage checking"
msgstr "corrige uma falha na verificação do uso de memória"

#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:101
msgid "First release for GNOME 40. Changes:"
msgstr "Primeiro lançamento para o GNOME 40. Alterações:"

#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:103
msgid "port to gnome-shell 40"
msgstr "port para gnome-shell 40"

#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:104
msgid "placeholder for gnome-shell prefs"
msgstr "espaço reservado para preferências do gnome-shell"

#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:112
#: data/appstream/org.gnome.GPaste.Ui.appdata.xml.in:176
